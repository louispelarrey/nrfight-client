{
  "crons": [{
    "path": "/api/cron/reserve",
    "schedule": "1 18 * * *"
  }],
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|svg|ico|css|js|json|woff|woff2|ttf|eot|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
